{
    "manifest_version": 3,
    "name":"Youtube Reloader Extention",
    "version":"1.0",
    "description":"Youtube Video Reloader",
    "permissions":["tabs","storage"],
    "background":{
        "service_worker":"background.js"
    },
    "host_permission":[
        "https://www.youtube.com/*"
    ],
    "action":{
        "default_popup":"popup.html"
    },
    "content_script":[{
        "matches":["https://www.youtube.com/*"],
        "js":["contnet.js"]
    }
    ]
}